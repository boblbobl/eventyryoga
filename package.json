{
  "name": "eventyryoga",
  "version": "1.0.0",
  "description": "eventyryoga.dk",
  "main": "index.html",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url src/sass/app.scss src/css/app.css",
    "css-watch": "npm run css-build -- --watch",
    "dev-server": "web-dev-server --node-resolve --open --watch",
    "build": "eleventy",
    "start": "npm run css-watch & eleventy --serve"
  },
  "author": "Lars Diget",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@web/dev-server": "^0.1.30",
    "bulma": "^0.9.3",
    "node-sass": "^7.0.1"
  }
}
