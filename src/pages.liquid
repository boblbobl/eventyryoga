---
pagination:
    data: contentful-page
    size: 1
    alias: page
    addAllPagesToCollections: true
permalink: "{{ page.slug | slugify }}.html"
tags: "page"
layout: base.liquid
eleventyComputed:
  title: "{{ page.title}}"
  subtitle: "{{ page.subtitle }}"
---

{%- for component in page.content -%}
  {% if component.sys.contentType.sys.id == "card" %}
    {% card component %}
  {% endif %}
{% endfor %}